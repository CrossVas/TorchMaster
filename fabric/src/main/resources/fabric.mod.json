{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/Xalcon/TorchMaster"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.xalcon.torchmaster.TorchmasterFabric"
        ],
        "client": [
            "net.xalcon.torchmaster.TorchmasterFabricClient"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=${java_version}"
    },
    "suggests": {
        "another-mod": "*"
    },
    "custom": {
        "modmenu": {
            "links": {
                "torchmaster.curseforge": "https://www.curseforge.com/minecraft/mc-mods/torchmaster",
                "torchmaster.modrinth": "https://modrinth.com/mod/torchmaster"
            }
        }
    }
}
  